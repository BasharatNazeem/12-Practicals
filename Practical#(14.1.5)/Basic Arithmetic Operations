#include<iostream>
using namespace std;
int add(int a, int b){
    return a+b;
}
int sub(int a,int b){
    return a-b;
}

int mul(int a,int b){
    return a*b;
}

int diV(int a,int b){
    return a/b;
}


int main(){
    int a,b,c;
    cout<<"enter a value for a ";
    cin>>a;
    cout<<"b: ";
    cin>>b;
    cout<<"addition= "<<add(a,b)<<endl;
    cout<<"subbtraction= "<<sub(a,b)<<endl;
    cout<<"multiplication= "<<mul(a,b)<<endl;
    cout<<"division= "<<diV(a,b)<<endl;
    
}
