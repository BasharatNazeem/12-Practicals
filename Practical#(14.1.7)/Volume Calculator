#include<iostream>
#include<cmath>
using namespace std;

double vol(double r,double h){
    return M_PI*(r*r)*h;
}

double vols(double R){
    return (4/3)*M_PI*(R*R*R);
}

double volc(double a){
    return a*a*a;
}


int main(){
    double r,h;
    cout<<"Radius of cylinder = ";
    cin>>r;
    cout<<"height of cylinder = ";
    cin>>h;
    cout<<vol(r,h)<<"\n\n\n";
    
    
    double R;
    cout<<"Radius of sphere = ";
    cin>>R;
    cout<<"Voulume of sphere is = "<<vols(R)<<"\n\n\n";
    
    
    double a;
    cout<<"enter value of one any side of cube = ";
    cin>>a;
    cout<<"volume of cube is "<<volc(a);
    
    
    
    
}
